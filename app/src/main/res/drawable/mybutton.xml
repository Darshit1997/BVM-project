<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#a19955" />
            <gradient android:angle="-90" android:startColor="#abae42" android:endColor="#cdd278"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#9c994b" />
            <solid android:color="#efef32"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="10dip" />
            <stroke android:width="1dip" android:color="#b3ab1e" />
            <gradient android:angle="-90" android:endColor="#dfd65b" android:startColor="#dede89" />
        </shape>
    </item>

</selector>